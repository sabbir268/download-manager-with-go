{
    "name": "download-manager",
    "version": "0.0.0",
    "license": "MIT",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "rebuild": "electron-rebuild -f -w electron",
        "package": "electron-packager . download-manager --platform=win32 --arch=x64 --version=0.25.0 --overwrite",
        "package-all": "electron-packager . download-manager --all --version=0.25.0 --overwrite",
        "package-win": "electron-packager . download-manager --platform=win32 --arch=x64 --version=0.25.0 --overwrite",
        "package-linux": "electron-packager . download-manager --platform=linux --arch=x64 --version=0.25.0 --overwrite",
        "package-mac": "electron-packager . download-manager --platform=darwin --arch=x64 --app-version=0.25.0 --overwrite"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.15",
        "electron": "^26.1.0",
        "postcss": "^8.4.29",
        "tailwindcss": "^3.3.3"
    },
    "dependencies": {
        "electron-context-menu": "^3.6.1",
        "electron-packager": "^17.1.2",
        "electron-rebuild": "^3.2.9"
    }
}
